{
  "name": "abraxas",
  "version": "1.0.0",
  "description": "A BreedTV media archive bot. It looks for videos posted to Slack and then saves them to Firestore.",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node-dev src/index.ts",
    "test": "ava",
    "build": "tsc",
    "serve": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waltz/abraxas.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/waltz/abraxas/issues"
  },
  "homepage": "https://github.com/waltz/abraxas#readme",
  "dependencies": {
    "@slack/bolt": "^3.12.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "get-urls": "^11.0.0",
    "install": "^0.13.0",
    "npm": "^9.3.1",
    "url-regex": "^5.0.0"
  },
  "devDependencies": {
    "@slack/types": "^2.8.0",
    "@types/express": "^4.17.15",
    "ava": "^5.2.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "ava": {
    "files": [
      "src/**/*_test*"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
